{
  "name": "roxana-map-react",
  "version": "0.0.1",
  "dependencies": {
    "classnames": "^2.2.3",
    "jquery": "^2.2.1",
    "leaflet": "^0.7.7",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-leaflet": "^0.10.1",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "bower": "^1.7.7",
    "browserify": "^13.0.0",
    "grunt": "^0.4.5",
    "grunt-bower": "^0.21.0",
    "grunt-cli": "^0.1.13",
    "http-server": "^0.9.0",
    "redux-thunk": "^1.0.3",
    "uglifyify": "^3.0.1"
  },
  "scripts": {
    "postinstall": "npm run setup-vendor",
    "presetup-vendor": "bower install",
    "setup-vendor": "grunt bower",
    "start": "npm run build-dev",
    "poststart": "http-server -a 127.0.0.1 -p 8100 --proxy http://127.0.0.1:8090 -o",
    "build-dev": "browserify -d -e src/app.js -t [ babelify --presets [ es2015 react ] ] -o public/scripts/build/bundle.dev.js",
    "build-min": "browserify -e public/scripts/build/bundle.dev.js -t uglifyify -o public/scripts/build/bundle.min.js"
  }
}
